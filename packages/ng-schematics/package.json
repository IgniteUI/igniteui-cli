    "@schematics/angular": "12.0.0",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.0.0",
    "typescript": "^4.2.4"
  },
  "ng-update": {
    "migrations": "./src/migrations/migration-collection.json",
