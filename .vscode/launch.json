{
	// Use IntelliSense to learn about possible Node.js debug attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch New jQuery",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": [ "${workspaceRoot}/lib/**/*.js",
				"${workspaceRoot}/spec/**/*.js" ],
			"args": [
				"new",
				"jqueryproj",
				"--framework=jquery"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch New Angular",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": [ "${workspaceRoot}/lib/**/*.js",
				"${workspaceRoot}/spec/**/*.js" ],
			"args": [
				"new",
				"angularproj",
				"--framework=angular"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Wizard",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": [ "${workspaceRoot}/packages/**/*.js",
				"${workspaceRoot}/spec/**/*.js" ]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch New React",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"new",
				"reactproj",
				"--framework=react"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Quickstart jQuery",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"quickstart",
				"--framework=jquery"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Quickstart React",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"quickstart",
				"--framework=react"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Quickstart Angular",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"quickstart",
				"--framework=angular"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Add",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"add"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Help",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"-h"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}/output/proj",
			"name": "Launch Start",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"start"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Test",
			"program": "${workspaceFolder}/spec/jasmine-runner.js",
			"preLaunchTask": "build",
			"outFiles": [ "${workspaceRoot}/lib/**/*.js",
				"${workspaceRoot}/spec/**/*.js" ]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Add Combo to Jquery",
			"cwd": "${workspaceRoot}/output/jqueryproj",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"add",
				"--template=combo",
				"--name=mycombo"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Add Combo to React",
			"cwd": "${workspaceRoot}/output/reactProj",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"add",
				"--template=combo",
				"--name=mycombo"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Add Combo to Angular",
			"cwd": "${workspaceRoot}/output/angularproj",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"add",
				"--template=combo",
				"--name=mycombo"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Wizard Reactproj",
			"cwd": "${workspaceRoot}/output/reactproj",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		},{
			"type": "node",
			"request": "launch",
			"name": "Launch Generate Template with name and framework",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"generate",
				"template",
				"--name=\"bla bla-bla\"",
				"--framework=jquery"
			]
		},{
			"type": "node",
			"request": "launch",
			"name": "Launch Config Get global",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"config",
				"set",
				"disableAnalytics",
				"false",
				"-g"
			]
		},{
			"type": "node",
			"request": "launch",
			"name": "Launch DOC Command, Term Alias",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"doc",
				"igGrid"
			]
		},{
			"type": "node",
			"request": "launch",
			"name": "Launch List command",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/packages/cli/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": [
				"list"
			]
		}, {
            "type": "node",
            "request": "launch",
			"cwd": "${workspaceRoot}/output",
            "name": "Launch NG CLI Schematic",
            "program": "${env:AppData}/npm/node_modules/@angular/cli/bin/ng",
            "outFiles": [
                "${workspaceFolder}/packages/ng-schematics/**/*.js"
            ],
            "args": [
                "new",
                "test3",
                "--collection=${workspaceFolder}/packages/ng-schematics/src/collection.json",
                "--dry-run"
            ],
			"preLaunchTask": "build",
            "console": "externalTerminal"
		}, {
            "type": "node",
            "request": "launch",
			"cwd": "${workspaceRoot}",
            "name": "Launch versioning script",
            "program": "${workspaceRoot}/versionScript.js",
			"preLaunchTask": "build",
            "console": "externalTerminal"
        }
	]
}